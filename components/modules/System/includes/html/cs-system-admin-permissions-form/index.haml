%polymer-element(name="cs-system-admin-permissions-form")
	%template
		.uk-form.uk-form-stacked
			%template(if="{{permission_id}}")
				%label.uk-form-label id
				%p [[permission_id]]
			%label.uk-form-label [[L.group]]
			%input(type="text" value="{{group}}")/
			%label.uk-form-label [[L.label]]
			%input(type="text" value="{{label}}")/
			%p
				%button.uk-button.uk-button-primary(on-click="[[save]]") [[L.save]]
	%script(src="script.js")
